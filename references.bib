@article{risk-potential-control,
author = {Hyatt-Twynam, Samuel R. and Parnell, Stephen and Stutt, Richard O. J. H. and Gottwald, Tim R. and Gilligan, Christopher A. and Cunniffe, Nik J.},
title = {Risk-based management of invading plant disease},
journal = {New Phytologist},
volume = {214},
