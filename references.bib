@article{CCA2,
title = "The connected-component labeling problem: A review of state-of-the-art algorithms",
journal = "Pattern Recognition",
volume = "70",
pages = "25 - 43",
year = "2017",
issn = "0031-3203",
doi = "https://doi.org/10.1016/j.patcog.2017.04.018",
