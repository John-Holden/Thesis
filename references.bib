@article{doi:10.1094/PHYTO.2003.93.12.1524,
author = {Coops, N. and Stanford, M. and Old, K. and Dudzinski, M. and Culvenor, D. and Stone, C.},
title = {Assessment of Dothistroma Needle Blight of Pinus radiata Using Airborne Hyperspectral Imagery},
